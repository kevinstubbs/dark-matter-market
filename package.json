{
  "name": "dark-matter-market",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "db:migrate": "pnpm --filter web db:migrate",
    "db:migrate:up": "pnpm --filter web db:migrate:up",
    "db:migrate:down": "pnpm --filter web db:migrate:down",
    "db:migrate:create": "pnpm --filter web db:migrate:create",
    "db:seed": "pnpm --filter web db:seed",
    "cache:topics": "pnpm --filter topic-cache-cli cache:topics",
    "agents:test": "pnpm --filter @dmm/agent-test test",
    "agents:buyer": "pnpm --filter @dmm/agents-buyer start",
    "agents:buyer:1": "cd agents/buyer && node dist/index.js configs/buyer_1.json",
    "agents:buyer:2": "cd agents/buyer && node dist/index.js configs/buyer_2.json",
    "agents:seller": "pnpm --filter @dmm/agents-seller start",
    "agents:seller:1": "cd agents/seller && node dist/index.js configs/seller_1.json",
    "agents:seller:2": "cd agents/seller && node dist/index.js configs/seller_2.json",
    "agents:seller:3": "cd agents/seller && node dist/index.js configs/seller_3.json",
    "agents:seller:4": "cd agents/seller && node dist/index.js configs/seller_4.json",
    "agents:seller:5": "cd agents/seller && node dist/index.js configs/seller_5.json",
    "agents:start-all": "tsx scripts/start-all-agents.ts"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "tsx": "^4.7.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}

